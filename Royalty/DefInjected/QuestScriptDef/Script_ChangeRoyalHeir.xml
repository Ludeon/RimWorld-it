  <!-- EN: Time left: {0} -->
  <ChangeRoyalHeir.InspectStringTimeLeft.slateRef>Tempo rimanente: {0}</ChangeRoyalHeir.InspectStringTimeLeft.slateRef>
  <!-- EN: [titleHolder_nameDef]'s heir change failed -->
  <ChangeRoyalHeir.LetterTextAskerOffended.slateRef>La modifica dell'erede di [titleHolder_nameDef] non è andata a buon fine</ChangeRoyalHeir.LetterTextAskerOffended.slateRef>
