<?xml version="1.0" encoding="UTF-8"?>
<LanguageData>
  
  <!-- EN: Follow master while the master is drafted. -->
  <FollowDrafted.headerTip>Folge Bezugsperson im Kampfmodus.</FollowDrafted.headerTip>
  
  <!-- EN: Follow master while the master is doing field work (hunting or taming animals). -->
  <FollowFieldwork.headerTip>Folge Bezugsperson bei Außeneinsätzen (Tiere jagen oder zähmen).</FollowFieldwork.headerTip>
  
  <!-- EN: Mark animals to be hunted by your hunters.\n\nOnly hunters with suitable ranged weapons will hunt. -->
  <Hunt.headerTip>Markiere Wildtiere, die von deinen Jägern gejagt werden sollen.\n\nNur Jäger mit geeigneten Fernkampfwaffen können auf die Jagd gehen.</Hunt.headerTip>
  
  <!-- EN: Release to wild -->
  <ReleaseAnimalToWild.headerTip>Auswildern</ReleaseAnimalToWild.headerTip>
  
  <!-- EN: Slaughter -->
  <Slaughter.headerTip>Schlachten</Slaughter.headerTip>
  
  <!-- EN: Sterilize -->
  <Sterile.headerTip>Sterilisieren</Sterile.headerTip>
  
  <!-- EN: Tame a wild animal. -->
  <Tame.headerTip>Zähme ein Wildtier.</Tame.headerTip>
  
</LanguageData>